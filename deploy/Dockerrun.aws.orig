{
    "AWSEBDockerrunVersion": "2",
    "authentication": {
        "bucket": "dockercredentials",
        "key": "docker/config.json"
    },
    "containerDefinitions": [{
        "name": "presentation",
        "image": "trinitymirror/presentation-service",
        "essential": "true",
        "memory": 256,
        "environment": [{
            "name": "ENV_SUFFIX",
            "value": "dev"
        }, {
            "name": "NODE_CONFIG",
            "value": "{}"
        }, {
            "name": "CONSUL_URL",
            "value": "http://ChronosConsul-dev.elasticbeanstalk.com:80/v1/kv/configmanifest"
        }],
        "portMappings": [{
            "hostPort": "80",
            "containerPort": "8080"
        }]
    }]
}